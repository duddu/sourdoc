    "version": "0.2.0",
    "configurations": [
        {
            "name": "sourdoc",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define",
                "VERSION=X.Y.ZZ",
                "--dart-define",
                "BUILD_NUMBER=1234567890",
                "--dart-define",
                "COMMIT_SHA=1234567"
            ]
        },
        {
            "name": "sourdoc (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "sourdoc (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "sourdoc (test mode)",
            "type": "dart",
            "request": "launch",
            "program": "./test/"
        }
    ],
}